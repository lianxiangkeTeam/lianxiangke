                android:layout_toRightOf="@+id/lian_xi_ke_fu_iv"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                style="@style/black_textview_mine"
                android:text="客服中心"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/arrow_rig"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/gray" />
            <RelativeLayout
                android:id="@+id/receiving_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:layout_gravity="center_vertical">
                <ImageView
                    android:id="@+id/mine_my_set_iv"
                    android:src="@mipmap/mine_my_set"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/mine_my_set_iv"
                    android:layout_marginLeft="15dp"
                    android:layout_centerVertical="true"
                    style="@style/black_textview_mine"
                    android:text="@string/receiving_address"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/arrow_rig"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="@color/gray" />
        </LinearLayout>
    </ScrollView>
</FrameLayout>
